{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "lib": ["ES2020", "WebWorker"],
        "target": "ES2020",
        "module": "ES2020",
        "moduleResolution": "Bundler",
        "noEmit": false,
        "outDir": "./public",
        "rootDir": "./src",
        "sourceMap": false,
        "declaration": false,
        "emitDeclarationOnly": false
    },
    "include": ["src/sw.ts"],
    "exclude": ["node_modules"]
}
